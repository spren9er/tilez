{
  "name": "tilez",
  "version": "0.1.1",
  "author": "Torsten Sprenger <mail@spren9er.de>",
  "description": "Generic layout engine for Svelte components",
  "keywords": [
    "svelte",
    "layout",
    "svg",
    "composition",
    "chart",
    "data visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spren9er/tilez.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spren9er/tilez/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "app:build": "vite build",
    "build": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "peerDependencies": {
    "svelte": "^4.1.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/package": "^2.2.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/svelte": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "publint": "^0.2.2",
    "regl": "^2.1.0",
    "svelte-check": "^3.5.1",
    "testing-library": "^0.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
