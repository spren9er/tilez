{
  "name": "tilez",
  "version": "0.1.3",
  "author": "Torsten Sprenger <mail@spren9er.de>",
  "description": "Generic layout engine for Svelte components",
  "keywords": [
    "svelte",
    "layout",
    "svg",
    "composition",
    "chart",
    "data visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spren9er/tilez.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spren9er/tilez/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "app:build": "vite build",
    "build": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "peerDependencies": {
    "svelte": "^4.2.9"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.2.7",
    "regl": "^2.1.0",
    "svelte-check": "^3.6.3",
    "testing-library": "^0.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
