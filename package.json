{
  "name": "tilez",
  "version": "0.0.32",
  "author": "Torsten Sprenger <mail@spren9er.de>",
  "description": "Generic layout engine for Svelte components",
  "keywords": [
    "svelte",
    "layout",
    "svg",
    "composition",
    "chart",
    "data visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spren9er/tilez.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spren9er/tilez/issues"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "app:build": "vite build",
    "build": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "peerDependencies": {
    "svelte": "^3.55.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.9.1",
    "@sveltejs/package": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitest/coverage-c8": "^0.29.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "publint": "^0.1.9",
    "regl": "^2.1.0",
    "svelte-check": "^3.0.4",
    "testing-library": "^0.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
